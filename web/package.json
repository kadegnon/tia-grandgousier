    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.4",
    "img-loader": "^2.0.1",
    "mocha": "^5.0.1",
    "sinon": "^4.4.2",
    "style-loader": "^0.20.2",
    "url-loader": "^1.0.1",
    "vue": "^2.5.13",
    "vue-loader": "^14.1.1",
    "webpack": "^4.1.0",
    "webpack-dev-server": "^3.1.0"
